 SUBROUTINE rpart(jl,jk,krow,a,f,x0,deltat)                                                                                         
                                                                                                                                    
    ! Re-calculates heterogenous reaction coefficients depending on HCl                                                             
    ! for current Newton-Raphson step and calculates Jacobian matrix for the                                                        
    ! Newton-Raphson method.                                                                                                        
                                                                                                                                    
    ! *rpart* is called from *newraph*, src/socol_nrd.f90.                                                                          
                                                                                                                                    
    ! Subroutine arguments:                                                                                                         
    INTEGER, INTENT(in)   :: jl, jk, krow                                                                                           
    REAL(dp), INTENT(in)  :: deltat                                                                                                 
    REAL(dp), INTENT(in)  :: x0(ngr)                                                                                                
    REAL(dp), INTENT(out) :: f(ngr), a(ngr,ngr)                                                                                     
                                                                                                                                    
    ! Local variables:                                                                                                              
    REAL(dp) :: SHS2P, HS1P, HS2P, HS3P, SHS2M,  HS1M, HS2M, HS3M, SHS2D, &                                                         
         HS1D, HS2D, HS3D, HP1D, HP2D, HP4D,  HP5D, HP6D                !MSHET2                                                     
    REAL(dp), PARAMETER :: dx = 0.02_dp     !increment of HCL for derivation                                                        
                                                                                                                                    
                                                                                                                                    
    ! Executable statements:                                                                                                        
                                                                                                                                    
    ! Re-calculate heterogeneous reaction coefficients depending on HCl                                                             
    ! for current HCl concentration and determine derivation with respect to HCl                                                    
    ! of these coefficients:                                                                                                        
    IF (lhetchem .AND. pscliqtest .GE. 1) THEN                         !MSHET2                                                      
       ! Heterogenous reaction coefficients  depending on HCl:                                                                      
       CALL calc_hetre(jl,jk,krow,HCL,SHS2,HS1,HS2,HS3)                                                                             
                                                                                                                                    
       ! Derivations with respect to HCl of heterogeneous reaction coefficients:                                                    
       IF (HCL .GE. 1.0_dp) THEN                                                                                                    
          CALL calc_hetre &                                                                                                         
               (jl,jk,krow,(1.0_dp+0.5_dp*dx)*HCL,SHS2P,HS1P,HS2P,HS3P)                                                             
          CALL calc_hetre &                                                                                                         
               (jl,jk,krow,(1.0_dp-0.5_dp*dx)*HCL,SHS2M,HS1M,HS2M,HS3M)                                                             
                                                                                                                                    
          SHS2D=(SHS2P-SHS2M)/(dx*HCL)                                                                                              
          HS1D=(HS1P-HS1M)/(dx*HCL)                                                                                                 
          HS2D=(HS2P-HS2M)/(dx*HCL)                                                                                                 
          HS3D=(HS3P-HS3M)/(dx*HCL)                                                                                                 
       ELSE                                                                                                                         
          SHS2D= 0.0_dp                                                                                                             
       ENDIF                                                                                                                        
    ELSE                                                                                                                            
       HS1 = 0.0_dp                                                                                                                 
       HS2 = 0.0_dp                                                                                                                 
       HS3 = 0.0_dp                                                                                                                 
       SHS1= 0.0_dp                                                                                                                 
       SHS2= 0.0_dp                                                                                                                 
       SHS3= 0.0_dp                                                                                                                 
                                                                                                                                    
       HS1D = 0.0_dp                                                                                                                
       HS2D = 0.0_dp                                                                                                                
       HS3D = 0.0_dp                                                                                                                
       SHS2D= 0.0_dp                                                                                                                
    ENDIF                                                                                                                           
                                                                                                                                    
    ! Heterogeneous reactions on ICE/NAT are independent on HCL:                                                                    
    HP1D = 0.0_dp                                                                                                                   
    HP2D = 0.0_dp                                                                                                                   
    HP4D = 0.0_dp                                                                                                                   
    HP5D = 0.0_dp                                                                                                                   
    HP6D = 0.0_dp                                                                                                                   
                                                                                                                                    
    ! Set heterogeneous reaction coefficients depending on HCl to zero if no                                                        
    ! HCl is present:                                                                                                               
    IF (HCL .LT. 1.0_dp) THEN                                                                                                       
       HS1 = 0.0_dp                                                                                                                 
       HS2 = 0.0_dp                                                                                                                 
       HS3 = 0.0_dp                                                                                                                 
       HS1D = 0.0_dp                                                                                                                
       HS2D = 0.0_dp                                                                                                                
       HS3D = 0.0_dp                                                                                                                
       HP1 = 0.0_dp                                                                                                                 
       HP2 = 0.0_dp                                                                                                                 
       HP4 = 0.0_dp                                                                                                                 
       HP5 = 0.0_dp                                                                                                                 
       HP6 = 0.0_dp                                                                                                                 
    ENDIF                                                                                                                           
                                                                                                                                    
    ! Set heterogeneous reaction coefficients depending on HCl to zero if no                                                        
    ! H2O is present:                                                                                                               
    IF (H2O .LT. 1.0_dp) THEN                                                                                                       
       SHS1 = 0.0_dp                                                                                                                
       SHS2 = 0.0_dp                                                                                                                
       SHS3 = 0.0_dp                                                                                                                
       SHS2D= 0.0_dp                                                                                                                
       SHP1 = 0.0_dp                                                                                                                
       SHP2 = 0.0_dp                                                                                                                
       SHP4 = 0.0_dp                                                                                                                
       SHP5 = 0.0_dp                                                                                                                
       SHP6 = 0.0_dp                                                                                                                
    ENDIF                                                                                                                           
                                                                                                                                    
   ! Calculate F(.) and Jacobian matrix for current  Newton-Raphson step:                                                           
                                                                                                                                    
!***** ***************************************                                                                                      
!*****       ELEMENT   1  O3                                                                                                        
!***** ***************************************                                                                                      
      F( 38)=X0(  1)-       O3+( &                                                                                                  
      -RO2 * O3 &                                                                                                                   
      -RO3 * O3 &                                                                                                                   
      -RO5 * OD * O3 &                                                                                                              
      -RO6 * O * O3 &                                                                                                               
      +RO7 * O * O2 * MU &                                                                                                          
      -RN2 * NO * O3 &                                                                                                              
      -RN7 * NO2 * O3 &                                                                                                             
      -RH3 * OH * O3 &                                                                                                              
      -RH6 * HO2 * O3 &                                                                                                             
      -RH9 * H * O3 &                                                                                                               
      -RC3 * CL * O3 &                                                                                                              
      -RB01 * BR * O3 &                                                                                                             
      -RT21 * OD * O3 &                                                                                                             
      -RTI02 * C5H8 * O3 &                                                                                                          
      -RTI11 * MACR * O3 &                                                                                                          
      +RTI24 * CH3CO3 * HO2 &                                                                                                       
      )*DELTAT                                                                                                                      
!***** ***************************************                                                                                      
!*****       ELEMENT   2  OD                                                                                                        
!***** ***************************************                                                                                      
      F(  6)=X0(  2)-       OD+( &                                                                                                  
      +RO3 * O3 &                                                                                                                   
      -RO4a * OD * N2 &                                                                                                             
      -RO4b * OD * O2 &                                                                                                             
      -RO5 * OD * O3 &                                                                                                              
      +RN17 * N2O &                                                                                                                 
      -RN18 * N2O * OD &                                                                                                            
      -RN19 * N2O * OD &                                                                                                            
      -RH1 * H2O * OD &                                                                                                             
      -RH16 * H2 * OD &                                                                                                             
      -RB16 * HBR * OD &                                                                                                            
      -RCH1 * CH4 * OD &                                                                                                            
      -RCH2 * CH4 * OD &                                                                                                            
      +RT02 * O2 &                                                                                                                  
      -RT13 * OD * CL2 &                                                                                                            
      -RT21 * OD * O3 &                                                                                                             
      -RT25 * OD * N2 * MU &                                                                                                        
      -RODODS01 * CFC11 * OD &                                                                                                      
      -RODODS02 * CFC12 * OD &                                                                                                      
      -RODODS03 * H1301 * OD &                                                                                                      
      -RODODS04 * CFC113 * OD &                                                                                                     
      -RODODS05 * CFC114 * OD &                                                                                                     
      -RODODS06 * CFC115 * OD &                                                                                                     
      -RODODS07 * CCL4 * OD &                                                                                                       
      -RODODS09 * HCFC22 * OD &                                                                                                     
      -RODODS10 * HCFC141B * OD &                                                                                                   
      -RODODS11 * HCFC142B * OD &                                                                                                   
      -RODODS12 * H1211 * OD &                                                                                                      
      -RODODS13 * CH3BR * OD &                                                                                                      
      -RODODS15 * HCFC21 * OD &                                                                                                     
      -RODODS16 * HCFC123 * OD &                                                                                                    
      -RODODS17 * H2402 * OD &                                                                                                      
      -RODODS18 * CHBR3 * OD &                                                                                                      
      -RODODS19 * CH2BR2 * OD &                                                                                                     
      )*DELTAT                                                                                                                      
!***** ***************************************                                                                                      
!*****       ELEMENT   3  O                                                                                                         
!***** ***************************************                                                                                      
      F( 44)=X0(  3)-        O+( &                                                                                                  
      +RO1 * O2 &                                                                                                                   
      +RO1 * O2 &                                                                                                                   
      +RO2 * O3 &                                                                                                                   
      +RO4a * OD * N2 &                                                                                                             
      +RO4b * OD * O2 &                                                                                                             
      -RO6 * O * O3 &                                                                                                               
      -RO7 * O * O2 * MU &                                                                                                          
      +RN1 * NO &                                                                                                                   
      +RN5 * NO2 &                                                                                                                  
      -RN6 * NO2 * O &                                                                                                              
      +RN14 * NO3 &                                                                                                                 
      -RN15a * NO3 * O &                                                                                                            
      -RN24 * CLNO3 * O &                                                                                                           
      +RN26 * N * O2 &                                                                                                              
      +RN28 * N * NO &                                                                                                              
      +RN29 * N * NO2 &                                                                                                             
      -RH2 * OH * O &                                                                                                               
      +RH13 * OH * OH &                                                                                                             
      -RH5 * HO2 * O &                                                                                                              
      -RH12a * H2O2 * O &                                                                                                           
      -RC5 * CLO * O &                                                                                                              
      -RC26 * HCL * O &                                                                                                             
      -RB06 * BRO * O &                                                                                                             
      -RB15 * HBR * O &                                                                                                             
      -RB17 * HOBR * O &                                                                                                            
      +RB19 * BRO &                                                                                                                 
      -RCH7a * CH2O * O &                                                                                                           
      +RCH12 * CO2 &                                                                                                                
      +RT02 * O2 &                                                                                                                  
      -RT06 * H2 * O &                                                                                                              
      -RT11 * O * HOCL &                                                                                                            
      -RT17 * O * HCL &                                                                                                             
      +RT21 * OD * O3 &                                                                                                             
      +RT21 * OD * O3 &                                                                                                             
      -RT22 * O * O * MU &                                                                                                          
      -RT22 * O * O * MU &                                                                                                          
      -RT23 * NO * O * MU &                                                                                                         
      -RT24 * NO2 * O * MU &                                                                                                        
      )*DELTAT                                                                                                                      
!***** ***************************************                                                                                      
!*****       ELEMENT   4  NO                                                                                                        
!***** ***************************************                                                                                      
      F( 41)=X0(  4)-       NO+( &                                                                                                  
      -RN1 * NO &                                                                                                                   
      -RN2 * NO * O3 &                                                                                                              
      -RN3 * NO * HO2 &                                                                                                             
      -RN4 * NO * CLO &                                                                                                             
      +RN5 * NO2 &                                                                                                                  
      +RN6 * NO2 * O &                                                                                                              
      +RN15 * NO3 &                                                                                                                 
      -RN15b * NO3 * NO &                                                                                                           
      +RN18 * N2O * OD &                                                                                                            
      +RN18 * N2O * OD &                                                                                                            
      +RN26 * N * O2 &                                                                                                              
      +RN27 * N * HO2 &                                                                                                             
      -RN28 * N * NO &                                                                                                              
      +RN31 * N * OH &                                                                                                              
      -RB10 * BRO * NO &                                                                                                            
      -RCH5 * CH3O2 * NO &                                                                                                          
      +RT20 * H * NO2 &                                                                                                             
      -RT23 * NO * O * MU &                                                                                                         
      -RTI04 * ISO2 * NO &                                                                                                          
      -RTI05 * ISO2 * NO &                                                                                                          
      -RTI12 * MACRO2 * NO &                                                                                                        
      -RTI25 * CH3CO3 * NO &                                                                                                        
      )*DELTAT                                                                                                                      
!***** ***************************************                                                                                      
!*****       ELEMENT   5  HO2                                                                                                       
!***** ***************************************                                                                                      
      F( 48)=X0(  5)-      HO2+( &                                                                                                  
      -RN3 * NO * HO2 &                                                                                                             
      +RN20 * HNO4 &                                                                                                                
      -RN22 * HO2 * NO2 * MU &                                                                                                      
      +RN22a * HNO4 * MU * MU &                                                                                                     
      -RN27 * N * HO2 &                                                                                                             
      +RH3 * OH * O3 &                                                                                                              
      -RH4 * OH * HO2 &                                                                                                             
      +RH11a * CO * OH * MU &                                                                                                       
      -RH5 * HO2 * O &                                                                                                              
      -RH6 * HO2 * O3 &                                                                                                             
      -RH7 * HO2 * HO2 &                                                                                                            
      -RH7 * HO2 * HO2 &                                                                                                            
      -RH7a * HO2 * HO2 &                                                                                                           
      -RH7a * HO2 * HO2 &                                                                                                           
      +RH12 * H2O2 * OH &                                                                                                           
      +RH12a * H2O2 * O &                                                                                                           
      +RH10 * H * O2 * MU &                                                                                                         
      -RH14 * H * HO2 &                                                                                                             
      -RH14a * H * HO2 &                                                                                                            
      +RC5a * CLO * OH &                                                                                                            
      -RC11 * CLO * HO2 &                                                                                                           
      -RC13 * CL * HO2 &                                                                                                            
      -RC13a * CL * HO2 &                                                                                                           
      +RC14 * CL * H2O2 &                                                                                                           
      -RB02 * BR * HO2 &                                                                                                            
      +RB05 * BR * H2O2 &                                                                                                           
      -RB13 * BRO * HO2 &                                                                                                           
      +RCH6 * CH3O * O2 &                                                                                                           
      +RCH10 * HCO * O2 &                                                                                                           
      -RCH13 * CH3O2 * HO2 &                                                                                                        
      +RCH21 * CH3O2 * CH3O2 &                                                                                                      
      +RT07 * NO3 * OH &                                                                                                            
      -RT08 * NO3 * HO2 &                                                                                                           
      -RT09 * NO3 * HO2 &                                                                                                           
      +RT10 * CH2O * NO3 &                                                                                                          
      +RT19 * BRO * OH &                                                                                                            
      +RTI02 *  0.250_dp * C5H8 * O3 &                                                                                              
      +RTI04 * ISO2 * NO &                                                                                                          
      -RTI06 * ISO2 * HO2 &                                                                                                         
      +RTI07 * ISO2 * ISO2 &                                                                                                        
      +RTI11 *  0.320_dp * MACR * O3 &                                                                                              
      +RTI12 *  0.750_dp * MACRO2 * NO &                                                                                            
      -RTI13 * MACRO2 * HO2 &                                                                                                       
      +RTI14 *  1.000_dp * MACRO2 * MACRO2 &                                                                                        
      +RTI19 * HACET * OH &                                                                                                         
      -RTI23 * CH3CO3 * HO2 &                                                                                                       
      -RTI24 * CH3CO3 * HO2 &                                                                                                       
      +RTI27 * CH3CO3 * CH3O2 &                                                                                                     
      +RTI35 * ISO2H &                                                                                                              
      +RTI36 * ISON &                                                                                                               
      +RTI37 * MACR &                                                                                                               
      +RTI39 *  1.000_dp * MACRO2H &                                                                                                
      +RTI40 * HACET &                                                                                                              
      +RTI41 * MGLY &                                                                                                               
      +RTI42 * NALD &                                                                                                               
      )*DELTAT                                                                                                                      
!***** ***************************************                                                                                      
!*****       ELEMENT   6  CLO                                                                                                       
!***** ***************************************                                                                                      
      F( 39)=X0(  6)-      CLO+( &                                                                                                  
      -RN4 * NO * CLO &                                                                                                             
      +RN24 * CLNO3 * O &                                                                                                           
      -RN25 * CLO * NO2 * MU &                                                                                                      
      +RC3 * CL * O3 &                                                                                                              
      -RC5 * CLO * O &                                                                                                              
      -RC5a * CLO * OH &                                                                                                            
      -RC5b * CLO * OH &                                                                                                            
      -RC11 * CLO * HO2 &                                                                                                           
