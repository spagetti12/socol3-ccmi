 !*****=======================================                                                                                      
      X0 =   2.500E-31_dp*(T300)**(  -1.800E+00_dp)                                                                                 
      X8 =   2.200E-11_dp*(T300)**(  -7.000E-01_dp)                                                                                 
      ST = 1.0_dp/((1.0_dp+(LOG10(X0*DEN/X8))**2))                                                                                  
      RT24            = X0*DEN/(1.0_dp+X0*DEN/X8)*0.6_dp**ST                                                                        
 !*****NO2+O+MU=NO3+MU                                                                                                              
 !*****=======================================                                                                                      
      RT25           =  2.800E-36_dp*(T300)**( -9.000E-01_dp)*DEN                                                                   
 !*****OD+N2+MU=N2O+MU                                                                                                              
 !*****=======================================                                                                                      
      X0 =   6.900E-31_dp*(T300)**(  -1.000E+00_dp)                                                                                 
      X8 =   2.600E-11_dp*(T300)**(   0.000E+00_dp)                                                                                 
      ST = 1.0_dp/((1.0_dp+(LOG10(X0*DEN/X8))**2))                                                                                  
      RT26            = X0*DEN/(1.0_dp+X0*DEN/X8)*0.6_dp**ST                                                                        
 !*****OH+OH+MU=H2O2+MU                                                                                                             
 !*****=======================================                                                                                      
      X0 =   9.700E-29_dp*(T300)**(  -5.600E+00_dp)                                                                                 
      X8 =   9.300E-12_dp*(T300)**(  -1.500E+00_dp)                                                                                 
      ST = 1.0_dp/((1.0_dp+(LOG10(X0*DEN/X8))**2))                                                                                  
      RTI15           = X0*DEN/(1.0_dp+X0*DEN/X8)*0.6_dp**ST                                                                        
 !*****MACRO2+NO2+MU=MPAN+MU                                                                                                        
 !*****=======================================                                                                                      
      RTI16           = RTI15          /(  9.000E-29_dp*EXP(  1.400E+04_dp/T))                                                      
 !*****MPAN+MU+MU=MACRO2+NO2+MU                                                                                                     
 !*****=======================================                                                                                      
      X0 =   9.700E-29_dp*(T300)**(  -5.600E+00_dp)                                                                                 
      X8 =   9.300E-12_dp*(T300)**(  -1.500E+00_dp)                                                                                 
      ST = 1.0_dp/((1.0_dp+(LOG10(X0*DEN/X8))**2))                                                                                  
      RTI26           = X0*DEN/(1.0_dp+X0*DEN/X8)*0.6_dp**ST                                                                        
 !*****CH3CO3+NO2+MU=PAN+MU                                                                                                         
 !*****=======================================                                                                                      
      RTI32           = RTI26          /(  9.000E-29_dp*EXP(  1.400E+04_dp/T))                                                      
 !*****PAN+MU+MU=CH3CO3+NO2+MU                                                                                                      
 !*****=======================================                                                                                      
                                                                                                                                    
   END SUBROUTINE fico                                                                                                              
                                                                                                                                    
                                                                                                                                    
